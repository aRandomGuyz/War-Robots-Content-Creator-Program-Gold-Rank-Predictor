  <script>

    let tooltipPinned = false;
let tooltipTimer;

document.addEventListener("mousemove", (e) => {
  const tooltip = document.getElementById("tooltip");

  if (!tooltipPinned && tooltip.style.display === "block") {
    tooltip.style.top = e.clientY + 15 + "px";
    tooltip.style.left = e.clientX + 15 + "px";
  }
});

document.addEventListener("mouseover", (e) => {
  if (e.target.classList.contains("tooltip-word")) {
    tooltipTimer = setTimeout(() => {
      showTooltip(e);
    }, 500); // 0.5 seconds
  }
});

document.addEventListener("mouseout", (e) => {
  if (e.target.classList.contains("tooltip-word")) {
    clearTimeout(tooltipTimer);
    if (!tooltipPinned) hideTooltip();
  }
});

document.addEventListener("click", (e) => {
  if (!e.target.classList.contains("tooltip-word") && tooltipPinned) {
    tooltipPinned = false;
    hideTooltip();
  }
});

function showTooltip(e) {
  const tooltip = document.getElementById("tooltip");
  tooltip.style.display = "block";
  tooltip.style.top = e.clientY + 15 + "px";
  tooltip.style.left = e.clientX + 15 + "px";
}

function hideTooltip() {
  document.getElementById("tooltip").style.display = "none";
}

function pinTooltip(e) {
  tooltipPinned = true;
  showTooltip(e);
}

    function toggleAbout() {
  const panel = document.getElementById("aboutPanel");
  const btn = document.getElementById("toggleAboutBtn");

  if (panel.style.display === "none") {
    panel.style.display = "block";
    btn.textContent = "‚ùå Hide About the Creator";
  } else {
    panel.style.display = "none";
    btn.textContent = "‚ÑπÔ∏è About the Creator";
  }
}

    let tenure = 0;

    function setTenure(value) {
      tenure = value;
      document.querySelectorAll('.tenure-options button').forEach(b => b.classList.remove('active'));
      document.querySelectorAll('.tenure-options button')[value - 1].classList.add('active');
    }

    function predict() {

      // üìÖ Auto-detect monthly reset (1st of month)
      const today = new Date();
      const isPeriodReset = today.getDate() >= 1 && today.getDate() <= 7;


      // Blocks prediction if stats are missing
if (
  videos.value === "" ||
  views.value === "" ||
  prevVideos.value === "" ||
  prevViews.value === ""
) {
  result.innerHTML = "‚ö†Ô∏è Please enter all current and previous period stats.";
  return;
}

      if (tenure === 0) {
  result.innerHTML = "‚ö†Ô∏è Please select your time in the Creator Program.";
  return;
}

      const v = Number(videos.value);
      const vw = Number(views.value);
      const pv = Number(prevVideos.value);
      const pvw = Number(prevViews.value);

      let score = 0;

      if (v >= pv) score += 2;
      if (vw >= pvw) score += 3;
      if (v >= 10) score += 2;
      if (vw >= 700) score += 3;

      let tenureModifier = 1;

if (tenure === 1) tenureModifier = 0.85; // new Silver = delayed trust
if (tenure === 2) tenureModifier = 1.0;  // normal
if (tenure === 3) tenureModifier = 1.1;  // long-term trust

score = Math.round(score * tenureModifier);

      score = Math.min(score, 13);

      // üîÑ AUTOMATIC PERIOD RESET OVERRIDE (Day 1 logic)
if (isPeriodReset) {

  let text = "";
  let prob = "";
  let window = "";
  let trendText = "üîÑ New monthly period ‚Äî judging based on last period";

  if (pv >= 20) {
    if (tenure === 1) {
      text = "‚ö†Ô∏è Strong last period, but new Silver under observation";
      prob = "55‚Äì70%";
      window = "Likely reviewed after more activity";
    } else if (tenure === 2) {
      text = "‚úÖ Strong momentum, promotion likely if activity resumes";
      prob = "65‚Äì80%";
      window = "Possible promotion this cycle";
    } else {
      text = "‚úÖ Strong momentum from proven creator";
      prob = "75‚Äì90%";
      window = "Very likely promotion at review";
    }
  }

  else if (pv >= 15) {
    if (tenure === 1) {
      text = "üü° Decent last period, but trust still forming";
      prob = "45‚Äì60%";
      window = "Needs early uploads";
    } else {
      text = "üü¢ Likely to perform well again";
      prob = "60‚Äì75%";
      window = "Promotion possible this cycle";
    }
  }

  else {
    text = "‚ö†Ô∏è Weak last period activity";
    prob = "30‚Äì45%";
    window = "Needs strong restart";
  }

  result.innerHTML = `
    <strong>${text}</strong><br>
    (${prob})<br><br>

    ‚è≥ <strong>Estimated Gold Window:</strong><br>
    ${window}<br><br>

    <div class="trend">${trendText}</div>
    <div class="trend">üìä Last Period Videos: ${pv}</div>
  `;
  return;
}

      // üìà Polished trend detection
let trendText = "üìâ Trend: Reduced Activity vs Last Period";

const videoDiff = v - pv;
const viewDiff = vw - pvw;

// üöÄ Massive effort increase
if (videoDiff >= 10) {
  trendText = "üöÄ Trend: Strong Improvement vs Last Period";
}

// üìà Clear improvement
else if (videoDiff >= 5) {
  trendText = "üìà Trend: Improved vs Last Period";
}

// üìà Balanced growth (both metrics up)
else if (videoDiff > 0 && viewDiff > 0) {
  trendText = "üìà Trend: Improved vs Last Period";
}

// ‚ûñ Roughly the same
else if (Math.abs(videoDiff) <= 1 && Math.abs(viewDiff) <= 100) {
  trendText = "‚ûñ Trend: Stable vs Last Period";
}

// ‚õî Major drop
else if (videoDiff <= -5) {
  trendText = "‚õî Trend: Sharp Decline vs Last Period";
}

// üìâ Slight drop (fallback)
else {
  trendText = "üìâ Trend: Reduced Activity vs Last Period";
}

// ‚è≥ Estimated Gold window (timing logic)
let window = "Future review";

if (score >= 10) {
  if (v >= 15 && pv >= 6 && tenure > 1) {
    window = `Likely promoted at <span class="tooltip-word" onclick="pinTooltip(event)">period boundary</span>`;
  } else if (v < 10 && tenure > 1) {
    window = "Promotion possible anytime";
  } else {
    window = "Next monthly review";
  }
} else if (score >= 7) {
  window = "Next 1‚Äì2 review cycles";
}


      let text = "‚ö†Ô∏è Staying Silver for now";
let prob = "40%";

if (score >= 10) {
  if (tenure === 1) {
    text = "‚ö†Ô∏è Strong stats, but new Silver (review likely delayed)";
    prob = "60‚Äì75%";
  } else if (tenure === 2) {
    text = "‚úÖ Good chance of reaching Gold";
    prob = "70‚Äì85%";
  } else {
    text = "‚úÖ Strong chance of reaching Gold";
    prob = "75‚Äì90%";
  }
}
else if (score >= 7) {
  if (tenure === 1) {
    text = "üü° Promising start, needs time in program";
    prob = "45‚Äì60%";
  } else {
    text = "üü° Possible, but needs consistency";
    prob = "55‚Äì70%";
  }
}


      result.innerHTML = `
  <strong>${text}</strong><br>
  (${prob})<br><br>

  ‚è≥ <strong>Estimated Gold Window:</strong><br>
  ${window}<br><br>

  <div class="trend">${trendText}</div>
  <div class="trend">üìä Consistency Score: ${score}/13</div>
`;

    }
    
  </script>


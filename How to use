  <script>
    function toggleAbout() {
  const panel = document.getElementById("aboutPanel");
  const btn = document.getElementById("toggleAboutBtn");

  if (panel.style.display === "none") {
    panel.style.display = "block";
    btn.textContent = "‚ùå Hide About the Creator";
  } else {
    panel.style.display = "none";
    btn.textContent = "‚ÑπÔ∏è About the Creator";
  }
}

    let tenure = 0;

    function setTenure(value) {
      tenure = value;
      document.querySelectorAll('.tenure-options button').forEach(b => b.classList.remove('active'));
      document.querySelectorAll('.tenure-options button')[value - 1].classList.add('active');
    }

    function predict() {
      // Blocks prediction if stats are missing
if (
  videos.value === "" ||
  views.value === "" ||
  prevVideos.value === "" ||
  prevViews.value === ""
) {
  result.innerHTML = "‚ö†Ô∏è Please enter all current and previous period stats.";
  return;
}

      if (tenure === 0) {
  result.innerHTML = "‚ö†Ô∏è Please select your time in the Creator Program.";
  return;
}

      const v = Number(videos.value);
      const vw = Number(views.value);
      const pv = Number(prevVideos.value);
      const pvw = Number(prevViews.value);

      let score = 0;

      if (v >= pv) score += 2;
      if (vw >= pvw) score += 3;
      if (v >= 10) score += 2;
      if (vw >= 700) score += 3;

      let tenureModifier = 1;

if (tenure === 1) tenureModifier = 0.85; // new Silver = delayed trust
if (tenure === 2) tenureModifier = 1.0;  // normal
if (tenure === 3) tenureModifier = 1.1;  // long-term trust

score = Math.round(score * tenureModifier);

      score = Math.min(score, 13);

      // üìà Trend detection
let trendText = "üìâ Trend: Decline vs last period";

if (v > pv && vw > pvw) {
  trendText = "üìà Trend: Improved vs last period";
} else if (v === pv && vw === pvw) {
  trendText = "‚ûñ Trend: Stable vs last period";
}

// ‚è≥ Estimated Gold window (timing logic)
let window = "Future review";

if (score >= 10) {
  if (v >= 15 && pv >= 6 && tenure > 1) {
    window = "Likely promoted at period boundary";
  } else if (v < 10 && tenure > 1) {
    window = "Promotion possible anytime";
  } else {
    window = "Next monthly review";
  }
} else if (score >= 7) {
  window = "Next 1‚Äì2 review cycles";
}


      let text = "‚ö†Ô∏è Staying Silver for now";
let prob = "40%";

if (score >= 10) {
  if (tenure === 1) {
    text = "‚ö†Ô∏è Strong stats, but new Silver (review likely delayed)";
    prob = "60‚Äì75%";
  } else if (tenure === 2) {
    text = "‚úÖ Good chance of reaching Gold";
    prob = "70‚Äì85%";
  } else {
    text = "‚úÖ Strong chance of reaching Gold";
    prob = "75‚Äì90%";
  }
}
else if (score >= 7) {
  if (tenure === 1) {
    text = "üü° Promising start, needs time in program";
    prob = "45‚Äì60%";
  } else {
    text = "üü° Possible, but needs consistency";
    prob = "55‚Äì70%";
  }
}


      result.innerHTML = `
  <strong>${text}</strong><br>
  (${prob})<br><br>

  ‚è≥ <strong>Estimated Gold Window:</strong><br>
  ${window}<br><br>

  <div class="trend">${trendText}</div>
  <div class="trend">üìä Consistency Score: ${score}/13</div>
`;

    }
  </script>


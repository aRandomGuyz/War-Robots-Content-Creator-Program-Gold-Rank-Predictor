  <script>
    function clamp(val, min, max) {
      return Math.max(min, Math.min(max, val));
    }

    function predict() {
      const videos = Number(document.getElementById("videos").value);
      const views = Number(document.getElementById("views").value);
      const subs = Number(document.getElementById("subs").value);

      const prevVideos = Number(document.getElementById("prevVideos").value);
      const prevViews = Number(document.getElementById("prevViews").value);

      // Baselines
      const targetVideos = 22;
      const targetViews = 1400;

      // Core score (current period only)
      const videoScore = clamp(videos / targetVideos, 0, 1);
      const viewScore = clamp(views / targetViews, 0, 1);
      const subScore = clamp(subs / 1200, 0, 1);

      let score =
        (videoScore * 0.35) +
        (viewScore * 0.50) +
        (subScore * 0.15);

      score = clamp(score, 0, 1);
      const probability = Math.round(score * 100);

      // Smooth month scaling
      let monthsToGold = Math.round(
        clamp((100 - probability) / 25, 0, 4)
      );

      const today = new Date();
      let estDate = new Date(
        today.getFullYear(),
        today.getMonth() + monthsToGold,
        5
      );

      // Prevent past dates
      if (estDate < today) {
        estDate = new Date(
          today.getFullYear(),
          today.getMonth() + monthsToGold + 1,
          5
        );
      }

      // Verdict
      let verdict;
      if (probability >= 80) {
        verdict = "ğŸ”¥ Extremely strong chance of reaching Gold";
      } else if (probability >= 65) {
        verdict = "âœ… Good momentum toward Gold";
      } else if (probability >= 50) {
        verdict = "âš ï¸ Possible, but needs consistency";
      } else {
        verdict = "âŒ Staying Silver for now";
      }

      // Trend (display only)
      let trendText = "";
      if (prevVideos > 0 && prevViews > 0) {
        if (videos > prevVideos && views > prevViews) {
          trendText = "ğŸ“ˆ Trend: Improved vs last period";
        } else if (videos < prevVideos && views < prevViews) {
          trendText = "ğŸ“‰ Trend: Decline vs last period";
        } else {
          trendText = "â– Trend: Mixed vs last period";
        }
      }

      document.getElementById("result").innerHTML = `
        <strong>${verdict}</strong><br>
        (${probability}% probability)<br><br>
        Estimated Gold date:<br>
        ${estDate.toDateString()}
        <div class="trend">${trendText}</div>
      `;
    }
  </script>

</body>
